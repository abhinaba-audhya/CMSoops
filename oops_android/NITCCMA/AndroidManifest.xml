<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nitccma"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <!-- Internet permission -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.nitccma.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.nitccma.WelcomeActivity"
            android:label="@string/title_activity_welcome" >
        </activity>
        <activity
            android:name="com.example.nitccma.Subjects"
            android:label="@string/title_activity_subjects" >
        </activity>
        <activity
            android:name="com.example.nitccma.SingleContactActivity"
            android:label="Single_Contact_View" >
        </activity>
        <activity
            android:name="com.example.nitccma.MainQueryDetailsList"
            android:label="Main_query_details" >
        </activity>
        <activity
            android:name="com.example.nitccma.Attendence"
            android:label="@string/title_activity_attendence" >
        </activity>
    </application>

</manifest>